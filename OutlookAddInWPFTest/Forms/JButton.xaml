<local:BaseWindow x:Class="OutlookAddInWPFTest.Forms.JButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
             xmlns:local="clr-namespace:OutlookAddInWPFTest.Forms.BaseForm"
             mc:Ignorable="d"
             Width="46" Height="46" 
             WindowStyle="None"
             AllowsTransparency="True"
             ResizeMode="NoResize"
             MouseDown="Window_MouseDown"
             PreviewMouseDown="Window_BeforeMouseDown"
             Topmost="True"
             d:DesignHeight="46" d:DesignWidth="46">
    <Window.Background>
        <SolidColorBrush Opacity="0.0" Color="White"/>
    </Window.Background>
    <svgc:SvgCanvas x:Name="ResourceSvgCanvas"
                    HorizontalAlignment="Center" VerticalAlignment="Center"/>
</local:BaseWindow>
